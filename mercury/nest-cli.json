{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mercury/src",
  "monorepo": true,
  "root": "apps/mercury",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/mercury/tsconfig.app.json",
    "plugins": [
      "@nestjs/graphql/plugin"
    ]
  },
  "projects": {
    "mercury": {
      "type": "application",
      "root": "apps/mercury",
      "entryFile": "main",
      "sourceRoot": "apps/mercury/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mercury/tsconfig.app.json"
      }
    },
    "mammon": {
      "type": "library",
      "root": "libs/mammon",
      "entryFile": "index",
      "sourceRoot": "libs/mammon/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mammon/tsconfig.lib.json"
      }
    },
    "s3-storage": {
      "type": "library",
      "root": "libs/s3-storage",
      "entryFile": "index",
      "sourceRoot": "libs/s3-storage/src",
      "compilerOptions": {
        "tsConfigPath": "libs/s3-storage/tsconfig.lib.json"
      }
    }
  }
}